{
  "_comment": "Автоматически генерируемые шаблоны URL для промоакций",
  "_description": "Этот файл создается и обновляется автоматически при добавлении примеров ссылок через бота",
  "okx": {
    "boost": {
      "pattern": "/ua/boost/x-launch/{navName}",
      "pattern_type": "path",
      "base_url": "https://web3.okx.com",
      "fields": {
        "navName": [
          "navName",
          "slug",
          "projectSlug",
          "projectCode"
        ]
      },
      "static_segments": [
        "ua",
        "boost",
        "x-launch"
      ]
    }
  },
  "bybit": {
    "token-splash": {
      "pattern": "/en/trade/spot/token-splash/detail?code={code}",
      "pattern_type": "query",
      "base_url": "https://www.bybit.com",
      "fields": {
        "code": [
          "code",
          "promoId",
          "campaignId",
          "id"
        ]
      },
      "static_segments": [
        "en",
        "trade",
        "spot",
        "token-splash",
        "detail"
      ]
    },
    "trade": {
      "pattern": "/en/trade/spot/token-splash/detail?code={code}",
      "pattern_type": "query",
      "base_url": "https://www.bybit.com",
      "fields": {
        "code": [
          "navName",
          "nav_name",
          "name",
          "title",
          "projectName",
          "tokenName",
          "coinName"
        ]
      },
      "static_segments": []
    }
  },
  "mexc": {
    "launchpad": {
      "pattern": "/ru-RU/launchpad/{activityCoin}/{launchpadId}",
      "pattern_type": "path",
      "base_url": "https://www.mexc.com",
      "fields": {
        "activityCoin": [
          "activityCoin",
          "token",
          "coin",
          "symbol"
        ],
        "launchpadId": [
          "launchpadId",
          "id",
          "campaignId",
          "projectId"
        ]
      },
      "static_segments": [
        "ru-RU",
        "launchpad"
      ]
    },
    "jggl": {
      "pattern": "/token-airdrop/{activityCurrency}/{id}",
      "pattern_type": "path",
      "base_url": "https://www.mexc.com",
      "fields": {
        "activityCurrency": [
          "activityCurrency",
          "activity_currency"
        ],
        "id": [
          "id",
          "_id"
        ]
      },
      "static_segments": [
        "token-airdrop"
      ]
    },
    "token-airdrop": {
      "pattern": "/ru-RU/token-airdrop/{activityCurrencyFullName}/{id}",
      "pattern_type": "path",
      "base_url": "https://www.mexc.com",
      "fields": {
        "activityCurrencyFullName": [
          "activityCurrencyFullName",
          "activity_currency_full_name",
          "name",
          "title",
          "projectName",
          "tokenName",
          "coinName"
        ],
        "id": [
          "id",
          "_id"
        ]
      },
      "static_segments": [
        "ru-RU",
        "token-airdrop"
      ]
    }
  },
  "gate.io": {
    "candy-drop": {
      "pattern": "/ru/candy-drop/detail/{currency}-{id}",
      "pattern_type": "composite",
      "base_url": "https://www.gate.com",
      "fields": {
        "currency": [
          "currency",
          "coin",
          "symbol",
          "token",
          "award_token"
        ],
        "id": [
          "id",
          "_id",
          "activityId",
          "campaignId"
        ]
      },
      "static_segments": [
        "ru",
        "candy-drop",
        "detail"
      ]
    }
  },
  "binance": {
    "simple-earn": {
      "pattern": "/earn/simple-earn/{asset}",
      "pattern_type": "path",
